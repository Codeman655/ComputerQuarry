path=shell.resolveProgram("_move")
if path==nil or not os.loadAPI(path) then
  print ("Can\'t load library: _move")
  return 1
end
--save starting coordinate
home_x, home_y, home_z=0,0,0
dest_x, dest_y, dest_z=0,0,0
orientation=1

_move.goto(dest_x, dest_z)
shell.run("excavate", area)
_move.goto(home_x, home_z)
_move.faceNort()
for i=1,9 do
  turtle.select(i)
  turtle.drop()
end
print("finished")
